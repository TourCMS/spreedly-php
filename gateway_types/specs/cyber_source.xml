<?xml version="1.0" encoding="UTF-8"?>
<gateway>
	<gateway_type>cyber_source</gateway_type>
	<name>CyberSource</name>
	<notes><![CDATA[<p>To add a <a href="http://www.cybersource.com" target="_blank">CyberSource gateway</a> you need to supply your CyberSource user name, and <a href="https://ebc.cybersource.com/ebc/accountmanagement/ToolkitSecuritySettingsLoad.do" target="_blank">a SOAP Toolkit secure Transaction Key generated in the CyberSource admin</a>.</p>]]></notes>
	<credentials>
		<credential>
			<name>user_name</name>
			<label>Your your CyberSource user name</label>
		</credential>
		<credential>
			<name>transaction_key</name>
			<label>SOAP Toolkit secure Transaction Key</label>
		</credential>
	</credentials>
</gateway>